{
  "proxy_url": {
    "description": "代理服务器地址（例如：http://127.0.0.1:7890）",
    "type": "string",
    "default": ""
  },
  "thumbnail_blur_level": {
    "description": "视频封面打码程度（0-100，0为不打码，100为完全打码）",
    "type": "int",
    "default": 50
  },
  "enable_thumbnail": {
    "description": "是否返回视频封面图片",
    "type": "bool",
    "default": true
  },
  "max_results": {
    "description": "最大返回结果数量",
    "type": "int",
    "default": 10
  },
  "timeout": {
    "description": "请求超时时间（秒）",
    "type": "int",
    "default": 30
  }
}